{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/",
      "destination": "/frameworks.json"
    },
    {
      "source": "/api/frameworks",
      "destination": "/frameworks.json"
    },
    {
      "source": "/api/v1/frameworks",
      "destination": "/frameworks.json"
    },
    {
      "source": "/api/examples/list",
      "destination": "/list.json"
    },
    {
      "source": "/api/examples/list-all",
      "destination": "/list-all.json"
    },
    {
      "source": "/api/examples/download/:slug.tar.gz",
      "destination": "/examples/:slug.tar"
    }
  ],
  "github": {
    "silent": true,
    "autoJobCancelation": true
  }
}
